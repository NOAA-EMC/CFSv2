#!/bin/bash
set -x

export machine=wcoss
export FC=ftn

export FFLAGSM=" -O3 -g -convert big_endian  -fp-model precise -align all -fno-alias -w -ftz -FR"
export LDFLAGSM=

export LIBSM="$W3EMC_LIB4 $W3NCO_LIB4 $BACIO_LIB $SIGIO_LIB"
export INCMOD=$SIGIO_INC 

echo; make=`basename $PWD`
echo make-ing ${make%.*}
echo

make -f Makefile
mv ${make%.*} ../../exec
rm -f *.o *.mod

