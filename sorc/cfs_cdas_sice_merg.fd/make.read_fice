SHELL=  /bin/sh
LIBS    = ${LIBSM}

FFLAGS=${FFLAGSM}

PROG=read_fice
SUF=cfs_cdas_

OBJS = fice_intp.o $(PROG).o

$(PROG): ${OBJS}
	$(FC) -o $(SUF)$(PROG) ${OBJS} ${LIBS} $(NCDF)

fice_intp.o: fice_intp.f
	$(FC) $(FFLAGS) $(INC) -c fice_intp.f

$(PROG).o:      $(PROG).f
	$(FC) $(FFLAGS) $(INC) -c $(PROG).f

