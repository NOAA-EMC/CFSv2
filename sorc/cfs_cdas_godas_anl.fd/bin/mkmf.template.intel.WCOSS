CFLAGS =
LDFLAGS = $(LIBS)

LIBS = $(NETCDF_LDFLAGS_F)   ###/usrx/local/NetCDF/3.6.3/lib/libnetcdf.a 
INCL = -I$(NETCDF_INC)       ###-I/usrx/local/NetCDF/3.6.3/include

