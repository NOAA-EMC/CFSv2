        254 |!BOC
        255 |!-----------------------------------------------------------------------
        256 |! Put together filename
        257 |!-----------------------------------------------------------------------
        258 |  write(unit=temp,fmt='(a40)') odir
        259 |  read(unit=temp,fmt='(80a1)') (fbase(i),i=1,80)
        260 |  c=0
