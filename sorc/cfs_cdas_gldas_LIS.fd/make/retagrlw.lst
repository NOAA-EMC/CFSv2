         23 |! !ROUTINE: retagrlw.F90
         24 |!
         25 |! !DESCRIPTION:
         26 |!  Opens, reads, interpolates and overlays LW radiation forcing.
         27 |!
         28 |!    TIME1 = most recent past data\\
         29 |!    TIME2 = most recent future data\\
         30 |!
         31 |!	1. Load AGRMET 3 level cloud amount (convertted from RTNEPH)\\
         32 |!	2. Convert 2 m forcing specific humidity to vapor pressure\\
         33 |!	3. Use AGRMET subroutine longwv() to calculate LW DOWN. Arguments are:\\
         34 |!	   cloud amount [$\%$], 2 m temperature [K], and 2 m vapor pressure [Pa]\\
         35 |!
         36 |! !REVISION HISTORY:
         37 |!  28 Oct 1999: Brian Cosgrove; Initial code
         38 |!  11 Apr 2000: Brian Cosgrove; changed code to use Forcing Mask (With inland
         39 |!               water filled in).  Deleted unused variables.
         40 |!  20 Jun 2000: Brian Cosgrove; changed code so that it uses LDAS%UDEF and
         41 |!                not a hard-wired undefined value of -999.9 and -999.0
         42 |!  25 Jun 2001: Urszula Jambor; Modified for AGRMET data use in GLDAS.
         43 |!  25 Oct 2001: Jesse Meng; Modified for AGRMET LW scheme implementation
         44 |!
         45 |! !INTERFACE:
         46 |subroutine retagrlw ( order,yr, mo, da, hr, ferror, flag )
         47 |! !USES:
         48 |  use lisdrv_module, only : lis, grid
         49 |  use obsradforcing_module, only : oblwdata1,oblwdata2
         50 |  use agrmetdomain_module, only : agrmetdrv
         51 |  use lis_openfilemod
         52 |  implicit none
         53 |! !ARGUMENTS:
         54 |  integer :: yr, mo, da, hr
         55 |  integer :: order              !retrieve data for time1 or time2
         56 |  integer :: ferror             !0=no radiation data found
         57 |                                !1=found observational data
         58 |!EOP
         59 |  integer, parameter :: nagrc=1440, nagrr=600
         60 |!=== Local Variables =====================================================
         61 |  integer :: i,j
         62 |  integer :: openerrN=0, openerrS=0 !set to non-zero if error found
         63 |  integer :: readerrN=0, readerrS=0 !set to non-zero if error found
         64 |  character*4  :: cyr
         65 |  character*2  :: cmo, cda, chr
         66 |  character*80 :: nameSH
         67 |
         68 |  integer :: flag               !data source, 1=AGRMET, all we have for now
         69 |  integer :: fvalid             !0=cloud data undef
         70 |
         71 |  integer :: c, r
         72 |
         73 |  real :: pdata1H(nagrc,nagrr)
         74 |  real :: pdata1M(nagrc,nagrr)
         75 |  real :: pdata1L(nagrc,nagrr)
         76 |
         77 |  real :: cldamtH(lis%d%ngrid) !high layer cloud amount [%]
         78 |  real :: cldamtM(lis%d%ngrid) !mid  layer cloud amount [%]
         79 |  real :: cldamtL(lis%d%ngrid) !low  layer cloud amount [%]
         80 |
         81 |  real :: cldamt1d( 3 )        !single column 3 layet cldamt [%]
         82 |
         83 |  real :: tair                 !2 m temperature [K]
         84 |  real :: vaporP               !2 m vapor pressure [Pa]
         85 |
         86 |  real :: rldown               !LW down [W/m2]
         87 |
         88 |!=== End Variable Definition =============================================
         89 |!BOC
         90 |!-------------------------------------------------------------------------
         91 |! If using AGRMET data, open, read in and interpolate AGRMET files
         92 |!   to appropriate GLDAS resolution;
         93 |! If error reading file, or data completely missing, ferror=0;
         94 |!------------------------------------------------------------------------
         95 |  ferror = 0
         96 |  do c = 1, lis%d%ngrid
         97 |     if (order == 1) then
         98 |        oblwdata1(c) = lis%d%udef
         99 |     else if (order == 2) then
        100 |        oblwdata2(c) = lis%d%udef
        101 |     end if
        102 |  end do
        103 |
        104 |  if (flag == 1) then
        105 |!------------------------------------------------------------------------
        106 |! 1. Generate AGRMET cloud filenames
        107 |! Load 3 layers cldamt data
        108 |!------------------------------------------------------------------------
        109 |
        110 |     write(cyr, '(I4.4)') yr
        111 |     write(cmo, '(I2.2)') mo
        112 |     write(cda, '(I2.2)') da
        113 |     write(chr, '(I2.2)') hr
        114 |
        115 |     nameSH = trim(agrmetdrv%agrmetdir)//'/CloudAGR/'//cyr//cmo// &
        116 |          '/cldamt_'//cyr//cmo//cda//chr
        117 |!     print*, 'Reading AGRMET file : ',nameSH
        118 |!     open(11, file=nameSH, status='old',access='direct',&
        119 |!          recl=3456000, iostat=openerrN)
        120 |     call lis_open_file(11, file=nameSH, status='old',access='direct',&
        121 |          recl=3456000, script='getagrmet_lw.pl')
        122 |#line 128
        128 |! Cluster's way
        129 |     read(11, rec=1,iostat=readerrN) pdata1L
        130 |     read(11, rec=2,iostat=readerrN) pdata1M
        131 |     read(11, rec=3,iostat=readerrN) pdata1H
        132 |
        133 |     close(11)
        134 |     if ((openerrN+readerrN) > 0) then
        135 |        ferror = 0
        136 |        print*, 'AGRMET file problem: ',nameSH
        137 |     else
        138 |        ferror = 1
        139 |     endif
        140 |     call interp_agrmet_lw( pdata1H,cldamtH, ferror )
        141 |     call interp_agrmet_lw( pdata1M,cldamtM, ferror )
        142 |     call interp_agrmet_lw(pdata1L,cldamtL, ferror )
        143 |     if ( ferror .EQ. 0 ) then
        144 |        call lis_log_msg('ERR: retagrlw -- ferror == 0')
        145 |        return
        146 |     endif
        147 |!----------------------------------------------------------------------
        148 |! If AGRMET cloud data is not undefined, set ferror=1
        149 |!----------------------------------------------------------------------
        150 |     fvalid = 0
        151 |     do c=1, lis%d%ngrid
        152 |        if (cldamtH(c) >= 0.0) fvalid = 1
        153 |        if (cldamtM(c) >= 0.0) fvalid = 1
        154 |        if (cldamtL(c) >= 0.0) fvalid = 1
        155 |     end do
        156 |     if ( fvalid .EQ. 0 ) then
        157 |        call lis_log_msg('ERR: retagrlw -- fvalid == 0')
        158 |        return
        159 |     endif
        160 |     do c=1,lis%d%ngrid
        161 |        rldown = 0.
        162 |           tair = grid(c)%forcing(1)
        163 |!----------------------------------------------------------------------
        164 |! 2. CONVERT 2 METER SPECIFIC HUMIDITY TO VAPOR PRESSURE
        165 |!----------------------------------------------------------------------
        166 |           vaporP = grid(c)%forcing(2) * grid(c)%forcing(7) / &
        167 |                    ( 0.622 + grid(c)%forcing(2) * (1-0.622) )	
        168 |!----------------------------------------------------------------------
        169 |! If tair, vaporP, and ALL 3 AGRMET LAYERS cldamt are defined,
        170 |! calculate rldown; transfer to proper output array
        171 |!----------------------------------------------------------------------
        172 |           fvalid = 1
        173 |	   if (tair.LT. 0.0) fvalid = 0
        174 |	   if (vaporP.LT. 0.0) fvalid = 0
        175 |           if (cldamtH(c) .LT. 0.0) fvalid = 0
        176 |           if (cldamtM(c) .LT. 0.0) fvalid = 0
        177 |           if (cldamtL(c) .LT. 0.0) fvalid = 0
        178 |
        179 |	   if (fvalid == 1) then
        180 |              cldamt1d(1) = cldamtL(c)
        181 |	      cldamt1d(2) = cldamtM(c)
        182 |	      cldamt1d(3) = cldamtH(c)
        183 |!----------------------------------------------------------------------
        184 |! 3. Calculate lw down
        185 |!----------------------------------------------------------------------
        186 |              call agrlwdn( tair, vaporp, cldamt1d, rldown )
        187 |              if (order==1) then
        188 |                 oblwdata1(c) = rldown
        189 |              else if (order==2) then
        190 |                 oblwdata2(c) = rldown
        191 |              end if
        192 |           else	
        193 |              if (order==1) then
        194 |                 oblwdata1(c) = lis%d%udef
        195 |              else if (order==2) then
        196 |                 oblwdata2(c) = lis%d%udef
        197 |              end if
        198 |           end if
        199 |        end do
        200 |     end if
        201 |  return
        202 |!EOC
        203 |end subroutine retagrlw
 
