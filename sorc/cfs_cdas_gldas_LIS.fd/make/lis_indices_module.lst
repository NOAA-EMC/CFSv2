         39 |
         40 |   implicit none
         41 |
         42 |   integer :: lis_nc_working ! number of longitude points for the running domain
         43 |   integer :: lis_nr_working ! number of latitude points for the running domain
         44 |
         45 |   integer :: lis_nc_data ! number of longitude points for the paramater
         46 |                          ! data domain
         47 |   integer :: lis_nr_data ! number of latitude points for the paramater
         48 |                          ! data domain
         49 |
         50 |   integer :: lis_tnroffset ! offset used to map a tile's global row number
         51 |                            ! to its local value
         52 |                            !
         53 |                            ! For a given process, say its patch of tiles
         54 |                            ! begins at global row number 18.  All local
         55 |                            ! arrays for this process will begin with an index
         56 |                            ! of 1.  This offset is used to adjust any
         57 |                            ! tile(i)%row references to local indices.
         58 |                            !
         59 |                            ! For example:
         60 |              ! noah(i)%albsf1 = value1(tile(i)%col, tile(i)%row-lis_tnroffset)
         61 |                            !
         62 |                            ! Currently there is no need for a column offset.
         63 |
         64 |   integer :: lis_grid_offset ! offset used to map a tile's global grid index
         65 |                              ! to its local value
         66 |                              !
         67 |                              ! See lis_tnroffset above.
         68 |
         69 |   integer :: lis_g2l_row_offset ! offset used to map a tile's global row
         70 |                                 ! index into its sub-domain row index
         71 |         !  ( running_southern_lat - parameter_data_southern_lat ) / resolution
         72 |                                 ! This offset take lis_tnroffset into account.
         73 |
         74 |   integer :: lis_g2l_col_offset ! offset used to map a tile's global column
         75 |                                 ! index into its sub-domain column index
         76 |         !  ( running_western_lon - parameter_data_western_lon ) / resolution
         77 |
