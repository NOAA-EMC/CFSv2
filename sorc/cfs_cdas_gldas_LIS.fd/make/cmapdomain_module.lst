        127 |! !ROUTINE: def_cmap_ip_input
        128 |!
        129 |! !DESCRIPTION:
        130 |!
        131 |! Calculates weights and neighbor information required for
        132 |! CMAP interpolation
        133 |!
        134 |! !INTERFACE:
        135 |  subroutine conserv_cmap_interp_input(gridDesci,gridDesco,npts)
        136 |    real, intent(in) :: gridDesci(50)
        137 |    real             :: gridDesco(50)
        138 |    real, parameter     :: fill = -9999.0
        139 |    real                :: xpts(npts), ypts(npts)
        140 |    real                :: xptb(npts), yptb(npts)
        141 |    real                :: rlob(npts), rlab(npts)
        142 |    integer             :: npts
        143 |    integer             :: ipopt(20)
        144 |    integer             :: nb1, nb2, mo
        145 |    integer             :: i1, i2, j1, j2
        146 |    real                :: xi, xf, yi, yf
        147 |    integer             :: get_fieldpos
        148 |    integer             :: iret,ib,nb,jb,n,nv,lb,wb
        149 |
        150 |    !  COMPUTE NUMBER OF OUTPUT POINTS AND THEIR LATITUDES AND LONGITUDES.
        151 |    mo = npts
        152 |    iret=0
        153 |    ipopt = 0
        154 |    ipopt(1) = -1
        155 |    ipopt(2) = -1
        156 |    if(gridDesco(1).ge.0) then
        157 |       call compute_coord(gridDesco, 0,mo,fill,xpts,ypts,rlon,rlat,nv,0)
        158 |       if(mo.eq.0) iret=3
        159 |    else
        160 |       iret=31
        161 |    endif
        162 |    ! - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
        163 |    !  SET PARAMETERS
        164 |    nb1=ipopt(1)
        165 |    if(nb1.eq.-1) nb1=2
        166 |    if(iret.eq.0.and.nb1.lt.0.) iret=32
        167 |    if(iret.eq.0.and.nb1.ge.20.and.ipopt(2).ne.-1) iret=32
        168 |    if(iret.eq.0) then
        169 |       nb2=2*nb1+1
        170 |       nb3=nb2*nb2
        171 |       nb4=nb3
        172 |       if(ipopt(2).ne.-1) then
        173 |          nb4=ipopt(2)
        174 |          do ib=1,nb1
        175 |             nb4=nb4+8*ib*ipopt(2+ib)
        176 |          enddo
        177 |       endif
        178 |    else
        179 |       nb2=0
        180 |       nb3=0
        181 |       nb4=0
        182 |    endif
        183 |
        184 |    ! - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
        185 |    !  LOOP OVER SAMPLE POINTS IN OUTPUT GRID BOX
        186 |    do nb=1,nb3
        187 |       !  locate input points and compute their weights
        188 |       jb=(nb-1)/nb2-nb1
        189 |       ib=nb-(jb+nb1)*nb2-nb1-1
        190 |       lb=max(abs(ib),abs(jb))
        191 |       wb=1
        192 |       if(ipopt(2).ne.-1) wb=ipopt(2+lb)
        193 |       if(wb.ne.0) then
        194 |          do n=1,mo
        195 |             xptb(n)=xpts(n)+ib/real(nb2)
        196 |             yptb(n)=ypts(n)+jb/real(nb2)
        197 |          enddo
        198 |          call compute_coord(gridDesco, 1,mo,fill,xptb,yptb,rlob,rlab,nv,0)
        199 |          call compute_coord(gridDesci,-1,mo,fill,xptb,yptb,rlob,rlab,nv,0)
        200 |          if(iret.eq.0.and.nv.eq.0.and.lb.eq.0) iret=2
        201 |          do n=1,mo
        202 |             xi=xptb(n)
        203 |             yi=yptb(n)
        204 |             if(xi.ne.fill.and.yi.ne.fill) then
        205 |                i1=xi
        206 |                i2=i1+1
        207 |                j1=yi
        208 |                j2=j1+1
        209 |                xf=xi-i1
        210 |                yf=yi-j1
        211 |                n11(n,nb)=get_fieldpos(i1,j1,gridDesci)
        212 |                n21(n,nb)=get_fieldpos(i2,j1,gridDesci)
        213 |                n12(n,nb)=get_fieldpos(i1,j2,gridDesci)
        214 |                n22(n,nb)=get_fieldpos(i2,j2,gridDesci)
        215 |                if(min(n11(n,nb),n21(n,nb),n12(n,nb),n22(n,nb)).gt.0) then
        216 |                   w11(n,nb)=(1-xf)*(1-yf)
        217 |                   w21(n,nb)=xf*(1-yf)
        218 |                   w12(n,nb)=(1-xf)*yf
        219 |                   w22(n,nb)=xf*yf
        220 |                else
        221 |                   n11(n,nb)=0
        222 |                   n21(n,nb)=0
        223 |                   n12(n,nb)=0
        224 |                   n22(n,nb)=0
        225 |                endif
        226 |             else
        227 |                n11(n,nb)=0
        228 |                n21(n,nb)=0
        229 |                n12(n,nb)=0
        230 |                n22(n,nb)=0
        231 |             endif
        232 |!             print*, 'def ',n,nb,n113(n,nb),n213(n,nb)
        233 |          enddo
        234 |       endif
        235 |    enddo
        236 |  end subroutine conserv_cmap_interp_input
