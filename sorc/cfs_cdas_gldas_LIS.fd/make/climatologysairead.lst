         39 |  use precision
         40 |!EOP
         41 |  implicit none
         42 |
         43 |!=== Arguments ===========================================================
         44 |  real :: tsai(lis%d%nch)
         45 |  real,allocatable :: domlai(:,:)
         46 |  integer :: cindex, rindex
         47 |  integer :: index
         48 |  real(r8) :: sai_t1_f(lis%d%nch)
         49 |  real(r8) :: sai_t2_f(lis%d%nch)
         50 |!=== Local variables
         51 |  integer            :: line,t,v,mlat,mlon,l
