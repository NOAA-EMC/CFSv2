        384 |! !ROUTINE: localtime
        385 |!
        386 |! !DESCRIPTION:
        387 |!
        388 |! Calculates the local time based on GMT
        389 |!
        390 |! !INTERFACE:
        391 |subroutine localtime (gmt,lon,lhour,zone)
        392 |! !ARGUMENTS:
        393 |  real:: gmt                 ! GMT time (0-23)
        394 |  real:: lon                 ! longitude in degrees
        395 |  real:: change              ! the change in number of hours between
        396 |  real:: lhour               ! local hour (0-23) 0= midnight, 23= 11:00 p.m.
        397 |
        398 |  integer::  i              ! working integer
        399 |  integer:: zone            ! time zone (1-24)
        400 |!EOP
        401 |!BOC
        402 |!----------------------------------------------------------------------
        403 |!  Determine into which time ZONE (15 degree interval) the
        404 |!  longitude falls.
        405 |!----------------------------------------------------------------------
        406 |  do i=1,25
        407 |     if (lon.lt.(-187.5+(15*i))) then
        408 |        zone=i
        409 |        if (zone.eq.25) zone=1
        410 |        go to 60
        411 |     end if
        412 |  end do
        413 |!----------------------------------------------------------------------
        414 |!     Calculate change (in number of hours) from GMT time to
        415 |!     local hour.  Change will be negative for zones < 13 and
        416 |!     positive for zones > 13.
        417 |!     There is also a correction for LHOUR < 0 and LHOUR > 23
        418 |!     to LHOUR between 0 and 23.
        419 |!----------------------------------------------------------------------
        420 |60 if (zone.lt.13) then
        421 |     change=zone-13
        422 |     lhour=gmt+change
        423 |  elseif (zone.eq.13) then
        424 |     lhour=gmt
        425 |  else
        426 |     change=zone-13
        427 |     lhour=gmt+change
        428 |  end if
        429 |  if (lhour.lt.0) lhour=lhour+24
        430 |  if (lhour.gt.23) lhour=lhour-24
        431 |  return
        432 |!EOC
        433 |end subroutine localtime
 
